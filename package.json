{
  "name": "roosevelt-closure",
  "description": "Google Closure compiler support for Roosevelt MVC web framework",
  "author": "Roosevelt Framework Team <rooseveltframework@gmail.com>",
  "contributors": [
    "Eric Newport <kethinov@gmail.com>",
    "Troy Coutu <autre31415@gmail.com>",
    "Alexander J. Lallier <alexanderlallier@aol.com>"
  ],
  "version": "0.5.3",
  "homepage": "https://github.com/rooseveltframework/roosevelt-closure",
  "license": "CC-BY-4.0",
  "main": "roosevelt-closure.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=10.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "google-closure-compiler-js": "20180610.0.0"
  },
  "devDependencies": {
    "c8": "~5.0.1",
    "codecov": "~3.5.0",
    "eslint": "~5.16.0",
    "eslint-plugin-mocha": "~5.3.0",
    "fs-extra": "~8.0.1",
    "husky": "~2.6.0",
    "lint-staged": "~8.2.1",
    "mocha": "~6.1.4",
    "roosevelt": "~0.14.4",
    "standard": "~12.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/roosevelt-closure.git"
  },
  "keywords": [
    "roosevelt.js",
    "googleclosure",
    "node.js",
    "closure",
    "compiler",
    "minifier",
    "minification"
  ],
  "preferGlobal": true,
  "_from": "roosevelt-closure@*",
  "gitHead": "",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/no-exclusive-tests": "error"
    }
  },
  "scripts": {
    "codecov": "./node_modules/.bin/codecov",
    "coverage": "c8 --reporter=text --reporter=lcov mocha --timeout 100000 test/unit",
    "lint": "standard && eslint ./test/unit",
    "test": "mocha --timeout 25000 test/unit"
  },
  "lint-staged": {
    "*.js": "standard"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && eslint ./test/unit"
    }
  }
}
