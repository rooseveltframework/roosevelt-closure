{
  "name": "roosevelt-closure",
  "description": "Google Closure compiler support for Roosevelt MVC web framework",
  "author": "Roosevelt Framework Team <rooseveltframework@gmail.com>",
  "contributors": [
    "Eric Newport <kethinov@gmail.com>",
    "Troy Coutu <autre31415@gmail.com>",
    "Alexander J. Lallier <alexanderlallier@aol.com>"
  ],
  "version": "0.5.1",
  "homepage": "https://github.com/rooseveltframework/roosevelt-closure",
  "license": "CC-BY-4.0",
  "main": "roosevelt-closure.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=8.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "google-closure-compiler-js": "20180506.0.0",
    "fs-extra": "~5.0.0"
  },
  "devDependencies": {
    "coveralls": "~3.0.0",
    "husky": "~0.14.0",
    "lint-staged": "~7.0.0",
    "mocha": "~5.1.0",
    "nyc": "~11.7.1",
    "roosevelt": "~0.11.8",
    "standard": "~11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/roosevelt-closure.git"
  },
  "keywords": [
    "roosevelt.js",
    "googleclosure",
    "node.js",
    "closure",
    "compiler",
    "minifier",
    "minification"
  ],
  "preferGlobal": true,
  "_from": "roosevelt-closure@*",
  "gitHead": "",
  "scripts": {
    "coverage": "nyc --reporter=html --reporter=text mocha --timeout 25000 --recursive test/unit",
    "coveralls": "nyc --reporter=lcov mocha --timeout 40000 --recursive test && cat ./coverage/lcov.info | coveralls",
    "test": " mocha --timeout 25000 --recursive test/unit && npm run standard",
    "precommit": "lint-staged",
    "standard": "standard"
  },
  "lint-staged": {
    "*.js": "standard"
  }
}
